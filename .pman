[general]
name = "Sixshooter Cats"
renderer = "none"
material_mode = "legacy"
physics_engine = "builtin"

[run]
main_file = "game/main.py"
auto_build = true
auto_save = true

[build]
asset_dir = "assets/"
export_dir = "game/assets/"
ignore_patterns = [ "*.blend1", "*.blend2", "cat*"]
converters = [ "blend2bam", "native2bam",]
